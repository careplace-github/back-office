{
  "name": "careplace-business",
  "author": "Careplace",
  "version": "0.1.5",
  "description": "Careplace Business - Software de gestão de operações para Empresas de Serviço de Apoio Domiciliário, Lares e Residências Sénior.",
  "private": true,
  "scripts": {
    "start": "ENV=prod next start -p 5001",
    "start:staging": "ENV=stag next start -p 5001",
    "start:development": "ENV=dev next start -p 5001",
    "build": "ENV=prod next build",
    "build:development": "ENV=dev next build",
    "build:staging": "ENV=stag next build",
    "dev": "ENV=dev next dev -p 5001",
    "dev:staging": "ENV=stag next dev -p 5001",
    "lint": "next lint",
    "lint:es": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "test:snyk": "snyk test",
    "dev:turbo": "next dev -p 5001 --turbo",
    "export": "yarn build && next export -o _static",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier-check": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'",
    "clear-all": "rm -rf .next .swc _static node_modules",
    "re-start": "rm -rf .next .swc _static node_modules && yarn install && yarn dev",
    "re-build": "rm -rf .next .swc _static node_modules && yarn install && yarn build",
    "test": "jest",
    "coverage": "jest --coverage",
    "e2e": "cypress open",
    "circular-deps": "npx madge --image graph.svg ./src",
    "depcheck": "depcheck --config=.depcheckrc"
  },
