# .snyk

# Specify the Snyk organization to use for testing and monitoring
org: a3842cbd-3843-4f7c-b070-9c3741901756

# Specify the project name for identification purposes
project: careplace/business-web

# Set the severity threshold for reporting vulnerabilities (optional)
# Can be "low", "medium", "high", or "critical"
severityThreshold: high

# Specify the base directory for the project (optional)
baseDir: .

# Specify the file path for the project manifest (optional)
file: package.json

# Specify the command to run to install dependencies (optional)
installCommand: yarn install